{
  "name": "webpack-react-mini-testbed",
  "description": "Minimal testbed for React with webpack",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf build",
    "serve": "webpack-dev-server --env=dev --open",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:prod": "webpack -p --env=prod --progress --profile --colors",
    "inline": "inliner build/index.html > build/index-inline.html",
    "build": "npm-run-all --serial clean build:prod inline",
    "dev": "npm run serve",
    "dist": "npm run build",
    "prod": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-core": "6.25.0",
    "babel-loader": "7.1.1",
    "babel-preset-es2015": "6.24.1",
    "clean-webpack-plugin": "0.1.16",
    "css-loader": "0.28.4",
    "extract-text-webpack-plugin": "3.0.0-rc.2",
    "html-webpack-plugin": "2.29.0",
    "inliner": "1.12.3",
    "npm-run-all": "4.0.2",
    "style-loader": "0.18.2",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.6.1",
    "babel-preset-react": "6.24.1",
    "react": "15.6.1",
    "react-dom": "15.6.1"
  }
}
